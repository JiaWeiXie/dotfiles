[color]
  ui = true
[core]
  editor = vim
[alias]
  l = log --color --graph --all --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
  a = add
  b = branch
  c = commit -v
  d = diff
  m = commit -v -m
  i = init
  p = push
  s = status
  aa = add .
  ac = add --all
  au = add -u
  cc = checkout
  cl = clone
  cm = checkout master
  ec = commit -v --amend
  pc = "!git push -u origin $(git branch --show-current)"
  pp = pull
  ss = show
[include]
  path = ~/.gitconfig.user
[includeIf "gitdir:~/Documents/projects/iinumbers/"]
  path = ~/.gitconfig.iinumbers
